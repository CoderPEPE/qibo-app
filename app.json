{
  "expo": {
    "name": "qibo-app",
    "slug": "qibo-app",
    "version": "1.0.0",
    "originalFullName": "@esophk/qibo-app",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "icon": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.render.qibo",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.render.qibo",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "googleServicesFile": "./firebase/google-services/android.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7b401cee-3442-435b-8ca6-7e45103d6550"
      }
    },
    "owner": "esophk",
    "scheme": "qibo-app",
    "plugins": [
      "@react-native-google-signin/google-signin"
    ]
  }
}
